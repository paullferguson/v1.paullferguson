DEF ConvertTextureT Script {
  eventIn   SFVec3f position
  eventOut  SFInt32 choice
  url ["vrmlscript:
      function position(value) {
	    x = value[0];
        y = value[2];

		picNum = 36;

		if (Math.abs(y) > 1e-15)
			angle = Math.atan(x/y);
		else
			angle = Math.PI/2.0;

		//angle += angle + Math.PI/2.0;
		if (y < 0)
			angle += Math.PI;

		if (angle < 0)
			angle += 2.0 * Math.PI;

		choice = Math.round(angle * ((picNum-1)/2.0/Math.PI));
	}
  "]
}






