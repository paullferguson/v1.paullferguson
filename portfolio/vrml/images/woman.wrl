DEF TableShadowMover_0 Script {
	eventIn SFVec3f position_in
	eventOut SFVec3f position_out
	field SFVec3f translation 2.5 -15 0
	field SFFloat x 0
	field SFFloat z 0
	field SFFloat y 0
	url "javascript:
		function position_in(value, time) {
		   translation = value;
			x = translation[0];
			z = translation[1];
			y = translation[2];
			position_out=new SFVec3f(x*1.07,z*1.05,y);
		}
	"
}


